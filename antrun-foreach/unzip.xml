<?xml version="1.0" encoding="UTF-8" ?>
<project name="maven-antrun-" default="main"  >
<target name="main">
  <exec executable="pwd">
                </exec>
  <taskdef resource="net/sf/antcontrib/antcontrib.properties"/>
  <foreach param="fileName" target="unzipLibs">
