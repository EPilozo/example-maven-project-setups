<?xml version="1.0" encoding="UTF-8"?>
<actions>
        <action>
            <actionName>run.single.main</actionName>
            <packagings>
                <packaging>*</packaging>
            </packagings>
            <goals>
                <goal>exec:exec</goal>
            </goals>
            <properties>
                
                <exec.executable>java</exec.executable>
                <exec.classpathScope>${classPathScope}</exec.classpathScope>
                <Env.RUN_THIS_CLASS>${packageClassName}</Env.RUN_THIS_CLASS>
            </properties>
        </action>
    </actions>
